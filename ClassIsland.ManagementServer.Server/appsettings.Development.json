{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ClassIsland.ManagementServer": "Trace"
    }
  },
  "ConnectionStrings": {
    "Development": "Host={Your Host};Uid={User Id};Password={Your Password};Database=classisland_management_dev",
    "Production": "Host={Your Host};Uid={User Id};Password={Your Password};Database=classisland_management"
  },
  "Kestrel": {
    "Endpoints": {
      "api": {
        "Url": "http://localhost:5090/",
        "Protocols": "Http1"
      },
      "grpc": {
        "Url": "http://localhost:5091/",
        "Protocols": "Http2"
      }
    }
  }
}
